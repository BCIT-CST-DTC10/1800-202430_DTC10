{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "gostudy",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/index",
        "destination": "/",
        "type": 301
      }, {
        "source": "/index.html",
        "destination": "/",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/login",
        "destination": "/login.html"
      }, {
        "source": "/profile",
        "destination": "/profile.html"
      }, {
        "source": "/detail",
        "destination": "/detail.html"
      }, {
        "source": "/review",
        "destination": "/review.html"
      }, {
        "source": "/map",
        "destination": "/map.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}

{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "gostudy",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/index",
        "destination": "/",
        "type": 301
      }, {
        "source": "/index.html",
        "destination": "/",
        "type": 301
      }, {
        "source": "/pages/index",
        "destination": "/",
        "type": 301
      }, {
        "source": "/pages/index.html",
        "destination": "/",
        "type": 301
      }, {
        "source": "/pages/login",
        "destination": "/login",
        "type": 301
      }, {
        "source": "/pages/login.html",
        "destination": "/login",
        "type": 301
      }, {
        "source": "/pages/profile",
        "destination": "/profile",
        "type": 301
      }, {
        "source": "/pages/profile.html",
        "destination": "/profile",
        "type": 301
      }, {
        "source": "/pages/search",
        "destination": "/search",
        "type": 301
      }, {
        "source": "/pages/search.html",
        "destination": "/search",
        "type": 301
      }, {
        "source": "/pages/detail",
        "destination": "/detail",
        "type": 301
      }, {
        "source": "/pages/detail.html",
        "destination": "/detail",
        "type": 301
      }, {
        "source": "/pages/review",
        "destination": "/review",
        "type": 301
      }, {
        "source": "/pages/review.html",
        "destination": "/review",
        "type": 301
      }, {
        "source": "/pages/map",
        "destination": "/map",
        "type": 301
      }, {
        "source": "/pages/map.html",
        "destination": "/map",
        "type": 301
      }, {
        "source": "/pages/create",
        "destination": "/create",
        "type": 301
      }, {
        "source": "/pages/create.html",
        "destination": "/create",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/pages/index.html"
      }, {
        "source": "/login",
        "destination": "/pages/login.html"
      }, {
        "source": "/profile",
        "destination": "/pages/profile.html"
      }, {
        "source": "/search",
        "destination": "/pages/search.html"
      }, {
        "source": "/detail",
        "destination": "/pages/detail.html"
      }, {
        "source": "/review",
        "destination": "/pages/review.html"
      }, {
        "source": "/map",
        "destination": "/pages/map.html"
      }, {
        "source": "/create",
        "destination": "/pages/create.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
